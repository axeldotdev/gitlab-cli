{
    "name": "axeldotdev/gitlab-cli",
    "description": "This is a CLI that helps you use the Gitlab GraphQL API.",
    "keywords": ["gitlab", "gitlab api", "gitlab cli", "graphql"],
    "type": "project",
    "homepage": "https://github.com/axeldotdev/gitlab-cli",
    "readme": "https://github.com/axeldotdev/gitlab-cli/blob/main/README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Charpentier",
            "email": "axelcharpentier0@icloud.com",
            "homepage": "https://github.com/axeldotdev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "axelcharpentier0@icloud.com",
        "source": "https://github.com/axeldotdev/gitlab-cli",
        "issues": "https://github.com/axeldotdev/gitlab-cli/issues",
        "docs": "https://github.com/axeldotdev/gitlab-cli/wiki",
        "chat": "https://github.com/axeldotdev/gitlab-cli/discussions"
    },
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/larastan": "^1.0",
        "nunomaduro/patrol": "^1.0",
        "nunomaduro/termwind": "^1.2",
        "pestphp/pest": "^1.3",
        "spatie/ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["gitlab-cli"]
}
